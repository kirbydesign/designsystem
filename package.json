{
  "name": "designsystem",
  "version": "1.0.2",
  "description": "Kirby Design Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kirbydesign/designsystem.git"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "Library",
    "Kirby"
  ],
  "author": "bdyjeo@bankdata.dk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kirbydesign/designsystem/issues"
  },
  "homepage": "https://cookbook.kirby.design",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build-polyfills": "webpack --config webpack.polyfills.config.js",
    "test": "ng test",
    "test:cookbook": "ng test cookbook",
    "test:designsystem": "ng test designsystem",
    "test:headless-browser": "ng test --browsers=ChromeCustom",
    "test:headless-browser:single": "npm run test:headless-browser -- --watch=false",
    "test:ci": "ng test --progress false --watch false --code-coverage",
    "lint": "npm run lint:cookbook && npm run lint:designsystem",
    "lint:cookbook": "npm run lint:cookbook:ts && npm run lint:cookbook:css",
    "lint:cookbook:ts": "tslint './apps/cookbook/src/**/*.ts'",
    "lint:cookbook:ts:fix": "npm run lint:cookbook:ts -- --fix",
    "lint:cookbook:css": "stylelint './apps/cookbook/src/**/*.scss'",
    "lint:cookbook:css:fix": "npm run lint:cookbook:css -- --fix",
    "lint:designsystem": "npm run lint:designsystem:ts && npm run lint:designsystem:css",
    "lint:designsystem:ts": "tslint './libs/designsystem/**/*.ts'",
    "lint:designsystem:ts:fix": "npm run lint:designsystem:ts -- --fix",
    "lint:designsystem:css": "stylelint './libs/designsystem/src/**/*.scss'",
    "lint:designsystem:css:fix": "npm run lint:designsystem:css -- --fix",
    "prettier": "npx prettier --check \"src/**/*.{ts,json,md,scss,html}\"",
    "prettier:fix": "npx prettier --write \"src/**/*.{ts,json,md,scss,html}\"",
    "e2e": "ng e2e",
    "dist": "npm run dist:cookbook",
    "dist:cookbook": "ng build cookbook --prod --aot --progress false",
    "predist:designsystem": "npm run extract-scss-variables",
    "dist:designsystem": "ng build designsystem",
    "postinstall": "npm run build-polyfills && npm run transpile:tools && npm run extract-scss-variables",
    "postversion": "git push && git push --tags",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "transpile:tools": "npm run transpile:tslint-rules && npm run transpile:sass-to-ts && npm run transpile:generate-mocks",
    "transpile:tslint-rules": "tsc -p tools/tslint-rules",
    "transpile:sass-to-ts": "tsc -p tools/sass-to-ts",
    "transpile:generate-mocks": "tsc -p tools/generate-mocks",
    "extract-scss-variables": "node ./scripts/sass-extract-to-ts.js",
    "generate-mocks": "node ./scripts/generate-mocks.js",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.10",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.10",
    "@angular/compiler": "8.2.10",
    "@angular/core": "8.2.10",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.2.10",
    "@angular/platform-browser": "8.2.10",
    "@angular/platform-browser-dynamic": "8.2.10",
    "@angular/router": "8.2.10",
    "@ionic/angular": "4.11.0",
    "@nrwl/angular": "^8.12.2",
    "@types/prismjs": "1.9.0",
    "core-js": "2.5.4",
    "firebase": "6.2.4",
    "highcharts": "7.0.3",
    "moment": "2.24.0",
    "prismjs": "1.15.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "8.3.9",
    "@angular/compiler-cli": "8.2.10",
    "@ngneat/spectator": "^4.10.0",
    "@nrwl/cypress": "8.12.2",
    "@nrwl/workspace": "8.12.2",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/jest": "^25.1.4",
    "@types/node": "8.9.4",
    "@types/webpack": "^4.41.6",
    "chokidar": "^3.3.1",
    "codelyzer": "^5.0.1",
    "coveralls": "3.0.2",
    "cypress": "^3.8.2",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "intersection-observer": "0.7.0",
    "is-ci": "^2.0.0",
    "is-docker": "1.1.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^9.4.2",
    "multimatch": "^4.0.0",
    "ng-mocks": "8.1.0",
    "ng-packagr": "^5.4.0",
    "node-sass": "^4.12.0",
    "post-npm-install": "^2.0.0",
    "prettier": "1.17.0",
    "raw-loader": "^3.1.0",
    "resize-observer-polyfill": "1.5.1",
    "sass-extract": "2.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "ts-node": "5.0.1",
    "tsickle": "^0.37.0",
    "tslint": "5.9.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3",
    "webpack-cli": "^3.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run generate-mocks && lint-staged",
      "pre-push": "npm run test:headless-browser:single",
      "post-merge": "post-npm-install"
    }
  }
}
