    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.14",
    "@netbasal/spectator": "3.8.2",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.5.0",
    "coveralls": "3.0.2",
    "cpx": "1.5.0",
    "is-docker": "1.1.0",
