{
  "name": "designsystem",
  "version": "0.8.25",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-polyfills": "webpack --config webpack.polyfills.config.js",
    "test": "ng test",
    "ci-test": "ng test --progress false --watch false --code-coverage",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint './src/**/*.ts'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint './src/**/*.scss'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "e2e": "ng e2e",
    "dist": "ng build --prod --aot --progress false",
    "pack-copy-to-dist-folder-watch": "cpx \"src\\kirby\\**\\!(*.spec.ts)\" dist-lib --watch",
    "postinstall": "npm run build-polyfills && npm run transpile:tools",
    "postversion": "git push && git push --tags",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "transpile:tools": "tsc -p tools/tslint-rules"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.10",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.10",
    "@angular/compiler": "8.2.10",
    "@angular/core": "8.2.10",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.2.10",
    "@angular/platform-browser": "8.2.10",
    "@angular/platform-browser-dynamic": "8.2.10",
    "@angular/router": "8.2.10",
    "@ionic/angular": "4.11.0",
    "@types/prismjs": "1.9.0",
    "core-js": "2.5.4",
    "firebase": "6.2.4",
    "highcharts": "7.0.3",
    "moment": "2.24.0",
    "prismjs": "1.15.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "8.3.9",
    "@angular/compiler-cli": "8.2.10",
    "@netbasal/spectator": "3.8.2",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "^5.0.1",
    "coveralls": "3.0.2",
    "cpx": "1.5.0",
    "husky": "^3.0.9",
    "is-docker": "1.1.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^9.4.2",
    "ng-mocks": "7.7.0",
    "node-sass": "^4.12.0",
    "post-npm-install": "^2.0.0",
    "prettier": "1.17.0",
    "raw-loader": "^3.1.0",
    "resize-observer-polyfill": "1.5.1",
    "sass-extract": "2.1.0",
    "sass-extract-loader": "1.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3",
    "webpack-cli": "^3.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "post-npm-install"
    }
  }
}
