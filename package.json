    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "@ionic/angular": "4.6.2",
    "@types/prismjs": "1.9.0",
    "core-js": "2.5.4",
    "firebase": "6.2.4",
    "highcharts": "7.0.3",
    "moment": "2.24.0",
    "prismjs": "1.15.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "6.3.3",
    "zone.js": "0.8.29"
  },
