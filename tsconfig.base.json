{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "kirbydesign/core/*": ["libs/core/*"],
    },
    "downlevelIteration": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "baseUrl": ".",
    "resolveJsonModule": true
  },
  "files": []
}
