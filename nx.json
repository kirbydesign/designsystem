{
  "affected": {
    "defaultBase": "stable"
  },
  "cli": {
    "analytics": false
  },
  "defaultProject": "designsystem",
  "generators": {
    "@nx/angular:application": {
      "style": "scss"
    },
    "@nx/angular:library": {
      "linter": "eslint"
    },
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "name": ".",
  "targetDefaults": {
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "build": {
      "inputs": ["production", "^production"],
      "cache": true
    },
    "test": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default"]
  },
  "parallel": 1
}
